{ "name": "Tone Tinter"
, "description": "Somewhat accurate, injectable tone classification"
, "manifest_version": 2
, "version": "1.0"
, "icons": 
    { "16": "corgi16.png"
    , "48": "corgi48.png"
    , "128": "corgi128.png" 
    }
, "permissions":
    [ "http://tone-analyzer-cs252.mybluemix.net/*"
    , "storage"
    ]
, "options_page": "options.html"
, "content_scripts":
    [ { "matches":
          [ "https://*/*"
          , "http://*/*"
          ]
      , "js": 
          [ "toneTinter.js"
          ]
      , "css": [ "toneTinter.css" ]
      , "run_at": "document_end"
      }
    ]
}
